# MongoDB Connection String from Render/MongoDB Atlas
MONGO_URI=mongodb+srv://<user>:<password>@<cluster>.mongodb.net/previsadb?retryWrites=true&w=majority

# JWT Secret Key (MUST be strong and long)
JWT_SECRET=YOUR_VERY_STRONG_AND_LONG_JWT_SECRET_KEY

# Your Vercel Frontend URL (CRITICAL for CORS)
FRONTEND_URL=https://previsa-mn-frontend.vercel.app

# Email for creating the first admin user (MUST match email used for login)
ADMIN_EMAIL=your.admin@email.com